def Multiply(x, y, z):
    Mul = x * y * z 
    return Mul
print("")
print ("Enter three number for Multiply: ")
first = int(input("Enter the first number: "))
second = int(input("Enter the second number: "))
thirt = int (input("Enter the thirt number: "))
print("The total number is: ", first, "*" , second , "*" , thirt , "=" , Multiply(first,second,thirt))
